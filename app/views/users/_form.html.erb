<div id="error_explanation">
<% if user.errors.any? %>
  <h3>looks like we have some errors:</h3>
  <ul>
    <% user.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  <% end %>
</div>
<div class="signup">
<%= form_for(user) do |f| %>
<%= f.label "first name" %>
<%= f.text_field :first_name %><br></br>
<%= f.label "last name" %>
<%= f.text_field :last_name %><br></br>
<%= f.label "budget($)" %>
<%= f.number_field :budget %><br></br>
<%= f.label "email" %>
<%= f.email_field :email %><br></br>
<%= f.label "password" %>
<%= f.password_field :password %><br></br>
<%= f.label "confirm password" %>
<%= f.password_field :password_confirmation %><br></br>
<%= f.button "submit" %>
<% end %>
</div>
